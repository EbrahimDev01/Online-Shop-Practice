@model List<TagForSelect>

@for (int i = 0; i < Model.Count; i++)
{
    <div class="w-100">
        <label asp-for="@Model[i].IsSelected">
            <input asp-for="@Model[i].IsSelected" />

            @Model[i].Title
        </label>

        <input asp-for="@Model[i].Id" hidden />
    </div>
} 